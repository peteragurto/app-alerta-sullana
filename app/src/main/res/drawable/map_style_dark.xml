<?xml version="1.0" encoding="utf-8"?>
<resources>
    <styleMap xmlns="http://schemas.android.com/res/styleable"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        map:version="8">

        <item name="map:background">
            <shape>
                <corners android:radius="0" />
                <solid android:color="#222222" />
            </shape>
        </item>

        <item name="map:water">
            <shape>
                <corners android:radius="0" />
                <solid android:color="#000000" />
            </shape>
        </item>

        <item name="map:land">
            <shape>
                <corners android:radius="0" />
                <solid android:color="#333333" />
            </shape>
        </item>

        <item name="map:roads">
            <shape>
                <corners android:radius="0" />
                <stroke android:color="#666666" android:width="1" />
            </shape>
        </item>

        <item name="map:buildings">
            <shape>
                <corners android:radius="0" />
                <solid android:color="#333333" />
            </shape>
        </item>

        <item name="map:labels">
            <shape>
                <corners android:radius="0" />
                <solid android:color="#ffffff" />
            </shape>
        </item>

    </styleMap>
</resources>